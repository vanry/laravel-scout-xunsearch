{
    "name": "vanry/laravel-scout-xunsearch",
    "description": "Xunsearch Driver for Laravel Scout",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "vanry",
            "email": "ivanry@163.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.4",
        "laravel/scout": "^3.0|^4.0",
        "hightman/xunsearch": "^1.4",
