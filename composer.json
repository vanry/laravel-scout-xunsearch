        "donatj/php-ini-builder": "^0.1.3"
    },
    "autoload": {
        "psr-4": {
            "Vanry\\Scout\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Vanry\\Scout\\XunsearchScoutServiceProvider"
            ]
        }
    }
